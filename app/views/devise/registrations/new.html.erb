<%= bootstrap_devise_error_messages! %>
<div class="panel panel-default devise-bs">
  <div class="panel-heading">
    <h4><%= t('.sign_up', default: 'Sign up') %></h4>
  </div>
  <div class="panel-body">
    <%= form_for(resource, as: resource_name, url: registration_path(resource_name), html: { role: 'form' }) do |f| %>
      <div class="form-group">
        <table>
          <tr>
            <td>
        <%= p "* " %><%= f.label :cedula %><%= p " - (la cÃ©dula 1.234.567-2 se ingresa 12345672)" %>
          </td>
          <td>
        <%= f.text_field :cedula, autofocus: true, class: 'form-control' %>
      </div>
      <div class="form-group">
        <%= p "* " %><%= f.label :email %>
        <%= f.email_field :email, autofocus: true, class: 'form-control' %>
      </div>      
      <div class="form-group">
        <%= f.label :nombre %>
        <%= f.text_field :nombre, autofocus: true, class: 'form-control' %>
      </div>
      <div class="form-group">
        <%= f.label :apellido %>
        <%= f.text_field :apellido, autofocus: true, class: 'form-control' %>
      </div>
      <div class="form-group">
        <%= f.label :direccion %>
        <%= f.text_field :direccion, autofocus: true, class: 'form-control' %>
      </div>
      <div class="form-group">
        <%= f.label :celular %>
        <%= f.text_field :celular, autofocus: true, class: 'form-control' %>
      </div>
      <div class="form-group">
        <%= f.label :alumno1 %>
        <%= f.text_field :alumno1, autofocus: true, class: 'form-control' %>
      </div>
      <div class="form-group">
        <%= f.label :alumno2 %>
        <%= f.text_field :alumno2, autofocus: true, class: 'form-control' %>
      </div>
      <div class="form-group">
        <%= f.label :alumno3 %>
        <%= f.text_field :alumno3, autofocus: true, class: 'form-control' %>
      </div>
      <%= f.submit t('.sign_up', default: 'Sign up'), class: 'btn btn-primary' %>
    <% end %>
  </div>
</div>
<%= render 'devise/shared/links' %>
